# WebSocket Terminal Server Configuration
PORT=3001
JWT_SECRET=your-jwt-secret-key-here

# Security Settings
MAX_CONNECTIONS=100
CONNECTION_TIMEOUT=300000
AUTH_TIMEOUT=30000

# Terminal Settings
DEFAULT_ROWS=24
DEFAULT_COLS=80
MAX_ROWS=200
MAX_COLS=500

# SSH Settings
SSH_TIMEOUT=10000
SSH_KEEPALIVE_INTERVAL=30000

# Logging
LOG_LEVEL=info
LOG_FILE=terminal-server.log